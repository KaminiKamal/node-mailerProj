<script>
$(document).ready(function() {
    $('#btn').click(function () {
      var data={
      	'usermail' : $("#usermail").val(),
        'topic' : $("#topic").val(),
        'description' : $("#description").val()
                    };
      console.log(data);
      	$.ajax({
        type:"POST",
        url:"/mymail",
        data :  data, 
        //dataType: 'json',  // multiple data sent using ajax
        success: function (result) {
           alert(result);
          
        }
        });
    });
    $('#btn1').click(function () {
      var data={
        
        'filename' : $("#fname").val()
                    };
